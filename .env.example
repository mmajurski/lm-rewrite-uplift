# Copy this file to .env and fill in your values.
# .env is gitignored and will never be committed.

# API key for your vLLM server (used by generation, evaluation, and inspect scripts)
VLLM_API_KEY=sk-no-key-required

# API key for a secondary vLLM endpoint (e.g. an inference chat service)
RCHAT_API_KEY=your-rchat-api-key-here

# OpenAI API key (only needed if using openai models)
OPENAI_API_KEY=sk-your-openai-key-here

# Grader model for inspect-ai evaluation scripts
# The model used to grade open-ended QA responses
GRADER_MODEL=v_llm/your-grader-model-name
GRADER_MODEL_BASE_URL=https://your-vllm-server:8443/v1
GRADER_MODEL_API_KEY=sk-no-key-required

# Embedding model for get_model_embeddings.py
EMBEDDING_MODEL=Qwen/Qwen3-30B-A3B-Instruct-2507
EMBEDDING_MODEL_BASE_URL=https://your-vllm-server:8444/v1
